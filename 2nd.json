{
  "widget": {
    "debug": "off",
    "window": {
      "title": "Another Widget Example",
      "name": "secondary_window",
      "width": 800,
      "height": 600
    },
    "image": {
      "src": "Images/Moon.png",
      "name": "moon1",
      "hOffset": 100,
      "vOffset": 100,
      "alignment": "left"
    },
    "text": {
      "data": "Press Here",
      "size": 24,
      "style": "italic",
      "name": "text2",
      "hOffset": 100,
      "vOffset": 200,
      "alignment": "left",
      "onMouseUp": "moon1.opacity = (moon1.opacity / 100) * 80;"
    }
  },
  "menu": {
    "id": "file",
    "value": "File",
    "popup": {
      "menuitem": [
        {
          "value": "New",
          "onclick": "CreateNewDoc()"
        },
        {
          "value": "Open",
          "onclick": "OpenDoc()"
        },
        {
          "value": "Close",
          "onclick": "CloseDoc()"
        }
      ]
    }
  }
}
